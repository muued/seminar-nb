@article{citeulike:9172967,
    abstract = {A simple method for testing the probability that a set of numbers is a sample from a known distribution consists of comparing the empirical cumulative distribution function of the sample, S<sub>n</sub>(x), with the known cumulative distribution function F(x). Both D<sub>n</sub> = maximum {S<sub>n</sub>(x) - F(x)} and D<sub>n</sub><sup>*</sup> = maximum |S<sub>n</sub>(x) - F(x)| are random variables, independent of the special form of F(x), if F(x) is continuous. This paper contains more extensive tables of the percentage points in the distributions of D<sub>n</sub> and D<sub>n</sub><sup>*</sup> than have been published previously. These values are obtained by empirical modification of a known asymptotic formula.},
    author = {Miller, Leslie H.},
    citeulike-article-id = {9172967},
